{
	"name": "AutoFit",
	"version": "1.0",
	"manifest_version": 2,
	"background": {
		"scripts": ["background.bundle.js"]
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.bundle.js"]
		}
	],
	"browser_action": {
		"default_popup": "index.html"
	},
	"permissions": ["tabs", "storage", "activeTab"],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
